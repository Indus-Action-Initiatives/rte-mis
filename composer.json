      },
      "drupal/pdf_serialization": {
        "3331105: Batch export support for Views data export module": "https://www.drupal.org/files/issues/2023-09-27/pdf_serialization-batch-export-support-3331105-9.patch"
      },
      "drupal/select2": {
        "select2: Entity reference based on the field value implemented.": "docroot/profiles/contrib/rte-mis/patches/select2_reference_specific_field_value.patch"
      },
      "drupal/simple_oauth": {
        "3457334: Fixed TokenAuthUser abstract methods": "https://git.drupalcode.org/project/simple_oauth/-/merge_requests/134.diff"
      },
      "drupal/user_history": {
        "3456158: Module not compatible with Profile": "https://git.drupalcode.org/project/user_history/-/merge_requests/8.diff"
      },
      "drupal/view_custom_table": {
        "3363668: Undefined array key 'column_relations'": "https://git.drupalcode.org/project/view_custom_table/-/merge_requests/23.diff"
      },
      "drupal/views_data_export": {
        "3204269: Batch export method needs a fix for pdf_serialization module": "https://www.drupal.org/files/issues/2023-09-27/views_data_export-batch-export-method-to-pdf-3204269-16.patch"
      }
    },
    "symlinks": {
      "config/content": "docroot/profiles/contrib/rte-mis/config/content",
      "config/install": "docroot/profiles/contrib/rte-mis/config/install",
      "config/optional": "docroot/profiles/contrib/rte-mis/config/optional",
      "modules": "docroot/profiles/contrib/rte-mis/modules",
      "patches": "docroot/profiles/contrib/rte-mis/patches",
      "rte_mis.ico": "docroot/profiles/contrib/rte-mis/rte_mis.ico",
      "rte_mis.info.yml": "docroot/profiles/contrib/rte-mis/rte_mis.info.yml",
      "rte_mis.install": "docroot/profiles/contrib/rte-mis/rte_mis.install",
      "rte_mis.profile": "docroot/profiles/contrib/rte-mis/rte_mis.profile",
      "src": "docroot/profiles/contrib/rte-mis/src",
      "themes": "docroot/profiles/contrib/rte-mis/themes"
    }
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks",
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-update-cmd": [
      "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks",
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-package-install": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFolders"
    ]
  }
}
