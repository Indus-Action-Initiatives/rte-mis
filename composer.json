    "phpoffice/phpspreadsheet": "^2.0",
    "select2/select2": "^4.0",
    "webflo/drupal-finder": "^1.3"
  },
  "require-dev": {
    "drupal/core-composer-scaffold": "10.2",
    "drush/drush": "^12",
    "innoraft/drupal-quality-checker": "^2.0",
    "kporras07/composer-symlinks": "^1.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "datatables/datatables",
        "type": "drupal-library",
        "version": "v1.10.21",
        "dist": {
          "type": "zip",
          "url": "https://github.com/DataTables/DataTables/archive/refs/heads/master.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "select2/select2",
        "type": "drupal-library",
        "version": "4.0.13",
        "dist": {
          "type": "zip",
          "url": "https://github.com/select2/select2/archive/refs/tags/4.0.13.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "jackmoore/colorbox",
        "type": "drupal-library",
        "version": "1.6.4",
        "dist": {
          "type": "zip",
          "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "DrupalProject\\composer\\": "scripts/composer/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true
    },
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
        "[project-root]/.editorconfig": false,
        "[project-root]/.gitattributes": false,
        "[web-root]/.csslintrc": false,
        "[web-root]/INSTALL.txt": false,
        "[web-root]/example.gitignore": false,
        "[web-root]/modules/README.txt": false,
        "[web-root]/profiles/README.txt": false,
        "[web-root]/robots.txt": false,
        "[web-root]/sites/README.txt": false,
        "[web-root]/themes/README.txt": false,
        "[web-root]/web.config": false
      },
      "locations": {
        "project-root": ".",
        "web-root": "./docroot"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patches": {
      "drupal/administerusersbyrole": {
        "Fixed the administerusersbyrole module issue for profiles": "docroot/profiles/contrib/rte-mis/patches/administerusersbyrole.patch",
        "issue with users view permission": "https://www.drupal.org/files/issues/2020-07-13/administerusersbyrole_edit_access-query-tag-alter-logic-fix-3158798-2.patch"
      },
      "drupal/autologout": {
        "3372010: Warning: Undefined array key 1 in AutologoutSubscriber->onRequest()": "https://git.drupalcode.org/project/autologout/-/merge_requests/34.diff"
      },
      "drupal/conditional_fields": {
        "Fix the role based condition issue": "https://git.drupalcode.org/project/conditional_fields/-/merge_requests/23.diff",
        "3344587: Support for field hidden by condition loosing its required status": "https://www.drupal.org/files/issues/2024-03-27/3344587-required-fields-that-59-with-fix.patch"
      },
      "drupal/core": {
        "2936995: Added revision tab for taxonomy": "https://www.drupal.org/files/issues/2023-10-26/add-taxonomy-revision-ui-2936995-58.patch",
        "3025384: Content translation overview should add destination query parameter": "https://www.drupal.org/files/issues/2022-02-14/3025384-31.patch",
        "3137947: Respect the 'limit validation errors' setting on AJAX request": "https://www.drupal.org/files/issues/2020-05-19/3137947-5-ajax-request-workflow.patch",
        "1091852: States manage on ajax call fixes ": "https://www.drupal.org/files/issues/2023-12-18/1091852-186.patch",
        "2928639: Set the expiration of a key/value pair from the PrivateTempStore": "https://www.drupal.org/files/issues/2021-07-27/2928639-26.patch",
        "1585930: Options module uses '_none' as a special value": "https://www.drupal.org/files/issues/2023-07-14/1585930-93.patch",
        "15624429: RuntimeException: Adding non-existent permissions to a role is not allowed": "https://www.drupal.org/files/issues/2024-06-03/3358586-36.patch",
        "1344902: Provide an optional destination parameter for local actions": "https://www.drupal.org/files/issues/2023-02-09/1344902-54.patch",
        "3061761: The language of content entities incorrectly falls back to the default language of the content whilst ignoring the current language of the user": "https://www.drupal.org/files/issues/2023-12-18/3061761-57.patch",
        "2449895: Imported PO files have no public URL but have a link at admin/content/files": "https://www.drupal.org/files/issues/2021-12-19/2449895_92.patch",
        "3403999: Exposed filter values ignored when using batch": "https://git.drupalcode.org/project/drupal/-/merge_requests/5883.diff",
        "1427854: More link' option should be configurable for views page displays": "https://www.drupal.org/files/issues/2023-04-18/1427854-94.patch",
        "3390178: big_pipe sometimes fails to load blocks": "https://git.drupalcode.org/project/drupal/-/commit/cb0a79958adf97108562ef722ca94d0e59d33a81.diff",
        "2912092: State required asterisk doesn't show": "https://www.drupal.org/files/issues/2023-08-18/2912092-67-10-1-x.patch"
      },
      "drupal/cshs": {
        "3270062: cshs filtering first level options": "https://www.drupal.org/files/issues/2023-03-22/3270062-problem-filtering-first-9.patch"
      },
      "drupal/entity_print": {
        "2971822: Allow users to choose whether to optimize css": "https://www.drupal.org/files/issues/2020-10-27/entity_print-allow_choose_optimize_css-14.patch"
      },
      "drupal/field_group": {
        "2969051: HTML5 Validation Prevents Submission in Tabs": "https://www.drupal.org/files/issues/2023-12-19/2969051-100_2.patch"
      },
      "drupal/filelog": {
        "3416342: Circular reference detected for service": "https://www.drupal.org/files/issues/2024-01-25/filelog_with_tests.patch"
      },
      "drupal/gin": {
        "Fix the dashboard back to site issue": "docroot/profiles/contrib/rte-mis/patches/gin_breadcrumbs.patch",
        "Fixed the breadcrumbs edit issue": "docroot/profiles/contrib/rte-mis/patches/gin_breadcrumb_edit_fix.patch",
        "Fixed the page template for gin admin theme": "docroot/profiles/contrib/rte-mis/patches/rtemis-node-custom-page.patch"
      },
      "drupal/lang_dropdown": {
        "3049683: Redirect to wrong URL when 'destination' query parameter is set": "https://www.drupal.org/files/issues/2023-03-20/redirect-to-wrong-url-when-destination-parameter-is-set-3049683-5.patch"
      },
      "drupal/menu_export": {
        "3263457: Fix the menu id null issue": "https://www.drupal.org/files/issues/2022-08-08/check-for-null-menu-name_3263457-2.patch",
        "3399829: Menu import broken issue": "https://www.drupal.org/files/issues/2024-03-11/menu-import-broken-in-d10-3399829.patch"
      },
      "drupal/mobile_number": {
        "Fix the full form validation issue": "https://www.drupal.org/files/issues/2023-08-20/add-limit-validation-errors-3273619-4.patch",
